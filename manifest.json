{
  "name": "Moctale Plus",
  "version": "1.3.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/logo.png",
      "32": "icons/logo.png",
      "48": "icons/logo.png",
      "128": "icons/logo.png"
    }
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "https://media.moctale.in/*",
    "https://www.moctale.in/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-829x50OV.js"
      ],
      "matches": [
        "https://moctale.in/*",
        "https://www.moctale.in/*"
      ],
      "run_at": "document_end",
      "css": [
        "assets/index-_rDlUPz4.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://moctale.in/*",
        "https://www.moctale.in/*"
      ],
      "resources": [
        "assets/index-D_Ii-jDN.js",
        "assets/index.tsx-mOKYForO.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
