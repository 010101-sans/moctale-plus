{
  "name": "Moctale Plus",
  "version": "1.9.8",
  "manifest_version": 3,
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/logo.png",
      "32": "icons/logo.png",
      "48": "icons/logo.png",
      "128": "icons/logo.png"
    }
  },
  "permissions": [
    "storage",
    "contextMenus",
    "identity",
    "alarms"
  ],
  "oauth2": {
    "client_id": "299859044236-62ulq62rf4hebtrk5h9b0mier8gkjrgq.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "https://media.moctale.in/*",
    "https://www.moctale.in/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-C8Hi_fKW.js"
      ],
      "matches": [
        "https://moctale.in/*",
        "https://www.moctale.in/*"
      ],
      "run_at": "document_end",
      "css": [
        "assets/index-mxIcULLP.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+M",
        "mac": "Alt+M"
      },
      "description": "Open Moctale Plus"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "icons/logo.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://moctale.in/*",
        "https://www.moctale.in/*"
      ],
      "resources": [
        "assets/vendor-B0imn2FR.js",
        "assets/index.tsx-ERjiqAr6.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "key": ""
}
